--osm-files osm.xml -o net.net.xml --opendrive-output foreign.net
--tls.discard-simple
--ramps.guess
--ramps.ramp-length 50
--keep-edges.by-type "highway.trunk,highway.primary_link,highway.trunk_link"
--geometry.remove
--roundabouts.guess
--junctions.join false
--osm.layer-elevation 6
--osm.oneway-spread-right
--junctions.scurve-stretch 0.8
--verbose
--geometry.check-overlap 3
--geometry.check-overlap.vertical-threshold 2
